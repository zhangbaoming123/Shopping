<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.lenovo.shopping">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/enjoy"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.ShoppingActivity"
            android:theme="@style/AppTheme"/>
        <activity android:name=".activity.SearchActivity" />
        <activity android:name=".activity.my.HeaderActivity" />
        <activity android:name=".activity.my.GuanActivity" />
        <activity android:name=".activity.my.JiFenActivity" />
        <activity android:name=".activity.my.YouActivity"></activity>
    </application>

</manifest>